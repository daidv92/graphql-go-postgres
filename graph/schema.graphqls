# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Member {
  id: ID!
  name: String
  skill: [Skill]
}

type Skill {
  id: ID!
  category: String
  name: String
  exp: String
}

type Query {
  members: [Member!]!
}

input NewMember {
  category: String
  name: String
  exp: String
}

type Mutation {
  createMember(input: NewMember!): Member!
}